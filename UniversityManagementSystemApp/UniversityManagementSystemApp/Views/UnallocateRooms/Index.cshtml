@{
    ViewBag.Title = "Unallocate Rooms";
}

<h2>Unallocate Rooms</h2>
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title></title>
</head>

<body>
    @if (ViewBag.message != null)
    {
        @ViewBag.message
    }


    <div>
        @using (Html.BeginForm())
        {
            <table>
                <tr>
                    <td>
                        <input type="submit" name="submit" value="Unallocate Rooms" class="btn btn-primary" onclick="return confirm('Are you sure to unallocate all the rooms?')" />
                    </td>
                </tr>


            </table>
        }
    </div>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
</body>
</html>

